{
    "jsc": {
        "parser": {
            "syntax": "typescript",
            "tsx": false,
            "decorators": true,
            "dynamicImport": true
        },
        "minify": {
            "compress": {
                "unused": true
            },
            "mangle": true
        },
        "target": "commonjs",
        "transform": {
            "optimizer": {
                "jsonify": {
                    "minCost": 0
                }
            }
        }
    },
    "minify": true,
    "module": {
        "type": "es6",
        "strict": true,
        "strictMode": true,
        "lazy": false,
        "noInterop": false
    }
}
